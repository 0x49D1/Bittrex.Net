<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net45;net46</TargetFrameworks>
  </PropertyGroup>
  
  <PropertyGroup>
    <Version>0.0.3</Version>
    <FileVersion>0.0.3</FileVersion>
    <Authors>JKorf</Authors>
    <Description>Bittrex .Net API wrapper</Description>
    <Copyright></Copyright>
  </PropertyGroup>
  
  <PropertyGroup>
    <PackageId>Bittrex.Net</PackageId>
	<Authors>JKorf</Authors>
    <PackageVersion>0.0.3</PackageVersion>
	<Description>Bittrex .Net API wrapper</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageTags>Bittrex Bittrex.Net CryptoCurrency Exchange API wrapper</PackageTags>
    <PackageProjectUrl>https://github.com/JKorf/Bittrex.Net</PackageProjectUrl>
	<PackageLicenseUrl>https://github.com/JKorf/Bittrex.Net/blob/master/LICENSE</PackageLicenseUrl>
	<NeutralLanguage>en</NeutralLanguage>
	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<PackageReleaseNotes>0.0.3 - * Small naming changes in socket client
	* Added api key checks in private endpoints
	* Updated documentation
	* Added example project</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNet.SignalR.Client" Version="2.2.2" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>
  
</Project>